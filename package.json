{
  "name": "general-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "npm run prisma:generate",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prebuild": "npm run prisma:generate",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate --schema prisma/portfolio.prisma && prisma generate --schema prisma/intervu.prisma",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate deploy",
    "db:init": "ts-node src/db/init-db.ts"
  },
  "dependencies": {
    "@graphql-tools/merge": "^9.0.6",
    "@apollo/server": "^4.11.0",
    "@prisma/client": "^5.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.1.0",
    "pg": "^8.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.6.6",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
